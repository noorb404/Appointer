    package="com.example.noor2.appointer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".WHOAREUActivity" />
        <activity android:name=".PickServiceActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".PaymentsActivty" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".AccountDetailsActivity" />
        <activity android:name=".CustomerProfileActivity" />
        <activity android:name=".BusinessProfileServices" />
        <activity android:name=".AddServiceActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".BusinessProfileDetails" />
        <activity android:name=".BusinessProfileProducts" />
        <activity android:name=".BusinessProfileHaircuts" />
        <activity android:name=".AppointerActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".OrderActivity" />
